<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.f5.mapper.MagazineHotContentMapper">
     <select id="selectMagazineHotByID" parameterType="Integer" resultType="magazinehotcontent">
     	select journalname,pubyear,pubvolume,pubphase from magazinereg where id=#{id};
     </select>
</mapper>